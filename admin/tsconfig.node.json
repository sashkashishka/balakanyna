{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "paths": {
      "@/*": [
        "./src/*"
      ],
    },
    "lib": [
      "ES2023"
    ],
    "module": "Preserve",
    /* Bundler mode */
    "moduleResolution": "bundler",
    "isolatedModules": true,
    "moduleDetection": "force"
  },
  "include": [
    "vite.config.ts"
  ]
}
